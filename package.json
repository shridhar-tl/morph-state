    "typescript": "^4.3.5",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.4"
  },
  "keywords": [
    "react",
    "state-management",
    "typescript",
    "proxy",
    "mutable",
    "mutable state"
