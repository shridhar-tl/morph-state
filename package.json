{
  "name": "morph-state",
  "version": "1.0.0",
  "description": "A fine-grained mutable state management library for React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shridhar-tl/morph-state.git"
  },
  "author": "Shridhar TL",
  "license": "MIT",
  "homepage": "https://github.com/shridhar-tl/morph-state.git#readme",
  "scripts": {
    "build": "webpack --mode production && dts-bundle-generator --out-file dist/index.d.ts --inline-declare-global src/index.ts",
